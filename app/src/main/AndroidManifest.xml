<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.illinois.cs.chara.charaapp" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activities.LoginActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.QueueActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_queue"/>
        <activity
            android:name=".activities.QueueListActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_queue_list"/>
        <activity
            android:name=".activities.StudentActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_student" >
        </activity>
    </application>

</manifest>
